#####
#checking the normal partial fit
######
#load leontief_2.R
#load modeling_leontief_1.R
#use rowsums as the data vector
statistic <- c()
levels_id_indicator <- c()
for(i in 1:15){
  statistic <- c(statistic,
                 unlist(rowsums_eachyear_leon_list[[i]][,1]))
  levels_id_indicator <- c(levels_id_indicator,
                           unlist(rowsums_eachyear_leon_list[[i]][,2]))
}

cbind(statistic,levels_id_indicator)

fit2 <- fit_wiod(statistic = statistic,
                 levels_id_vector = levels_id_indicator,
                 model = "normal",
                 pooling = "partial")
######
#checking the normal no pooling fit
######
fit3 <- fit_wiod(statistic = statistic,
                 levels_id_vector = levels_id_indicator,
                 model = "normal",
                 pooling = "none")

######
#checking the lognormal partial pooling fit
######
fit4 <- fit_wiod(statistic = statistic,
                 levels_id_vector = levels_id_indicator,
                 model = "lognormal",
                 pooling = "partial")
